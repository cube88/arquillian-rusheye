<?xml version="1.0" encoding="UTF-8"?>

<visual-suite xmlns="http://www.jboss.org/rusheye/visual-suite" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jboss.org/rusheye/visual-suite http://www.jboss.org/schema/arquillian/rusheye/visual-suite_0_2.xsd">
  <global-configuration>
    <listener type="org.jboss.rusheye.parser.listener.CompareListener">
      <result-collector>org.jboss.rusheye.parser.ManagerResultCollector</result-collector>
      <result-storage>org.jboss.rusheye.result.storage.FileStorage</result-storage>
      <result-writer>org.jboss.rusheye.result.writer.FileResultWriter</result-writer>
      <result-statistics>org.jboss.rusheye.result.statistics.OverallStatistics</result-statistics>
    </listener>
    <pattern-retriever type="org.jboss.rusheye.retriever.pattern.PatternFileRetriever"/>
    <mask-retriever type="org.jboss.rusheye.retriever.mask.MaskFileRetriever"/>
    <sample-retriever type="org.jboss.rusheye.retriever.sample.FileSampleRetriever"/>
    <perception/>
  </global-configuration>
  <case name="c2">
    <test name="AdvancedEditorTestCase.testAlignParagraph">
      <pattern name="AdvancedEditorTestCase.testAlignParagraph" source="c2/AdvancedEditorTestCase.testAlignParagraph.png"/>
    </test>
    <test name="AdvancedEditorTestCase.testBoldButton">
      <pattern name="AdvancedEditorTestCase.testBoldButton" source="c2/AdvancedEditorTestCase.testBoldButton.png"/>
    </test>
    <test name="AdvancedEditorTestCase.testFormat">
      <pattern name="AdvancedEditorTestCase.testFormat" source="c2/AdvancedEditorTestCase.testFormat.png"/>
    </test>
  </case>
  <case name="c1">
    <test name="ActionParameterTestCase.testSelectingNames">
      <pattern name="ActionParameterTestCase.testSelectingNames" source="c1/ActionParameterTestCase.testSelectingNames.png"/>
    </test>
    <test name="ActionParameterTestCase.testSelectingSkin">
      <pattern name="ActionParameterTestCase.testSelectingSkin" source="c1/ActionParameterTestCase.testSelectingSkin.png"/>
    </test>
    <test name="ActionParameterTestCase.testShowScreenSize">
      <pattern name="ActionParameterTestCase.testShowScreenSize" source="c1/ActionParameterTestCase.testShowScreenSize.png"/>
    </test>
  </case>
</visual-suite>
